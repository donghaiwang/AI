<?xml version="1.0" encoding="utf-8"?>
<CustomKeySet derivedfrom="Windows" modifieddefault="false">
   <Context id="Global">
      <Action id="select-previous-tab">
         <Stroke code="VK_PAGE_UP" ctrl="on"/>
         <Stroke code="VK_TAB" ctrl="on" shift="on"/>
      </Action>
      <Action id="change-current-folder">
         <Stroke alt="on" code="VK_F" shift="on"/>
         <Stroke code="VK_F" meta="on" shift="on"/>
      </Action>
      <Action id="select-next-tab">
         <Stroke code="VK_PAGE_DOWN" ctrl="on"/>
         <Stroke code="VK_TAB" ctrl="on"/>
      </Action>
      <Action id="split-screen-left-right">
         <Stroke alt="on" code="VK_S" shift="on"/>
         <Stroke code="VK_S" meta="on" shift="on"/>
      </Action>
      <Action id="publish-to-default-format">
         <Stroke code="VK_P" ctrl="on" shift="on"/>
      </Action>
      <Action id="goto-line">
         <Stroke code="VK_L" ctrl="on"/>
      </Action>
      <Action id="go-until-cursor">
         <Stroke alt="on" code="VK_R" shift="on"/>
         <Stroke code="VK_R" meta="on" shift="on"/>
      </Action>
      <Action id="show-preferences">
         <Stroke alt="on" code="VK_P" shift="on"/>
         <Stroke code="VK_P" meta="on" shift="on"/>
      </Action>
      <Action id="beep">
         <Stroke alt="on" code="VK_B" shift="on"/>
         <Stroke code="VK_B" meta="on" shift="on"/>
      </Action>
      <Action id="clear-workspace">
         <Stroke alt="on" code="VK_K" shift="on"/>
         <Stroke code="VK_K" meta="on" shift="on"/>
      </Action>
      <Action id="clear-all-breakpoints">
         <Stroke alt="on" code="VK_C" shift="on"/>
         <Stroke code="VK_C" meta="on" shift="on"/>
      </Action>
      <Action id="open-with-system-browser">
         <Stroke alt="on" code="VK_E" shift="on"/>
         <Stroke code="VK_E" meta="on" shift="on"/>
      </Action>
      <Action id="to-lower-case">
         <Stroke alt="on" code="VK_L" shift="on"/>
         <Stroke code="VK_L" meta="on" shift="on"/>
      </Action>
      <Action id="comment">
         <Stroke code="VK_R" ctrl="on"/>
         <Stroke code="VK_SLASH" ctrl="on"/>
      </Action>
      <Action id="clear-command-window">
         <Stroke alt="on" code="VK_W" shift="on"/>
         <Stroke code="VK_W" meta="on" shift="on"/>
      </Action>
      <Action id="uncomment">
         <Stroke code="VK_T" ctrl="on"/>
         <Stroke code="VK_COMMA" ctrl="on"/>
      </Action>
      <Action id="kill-line">
         <Stroke code="VK_K" ctrl="on"/>
      </Action>
      <Action id="set-path">
         <Stroke alt="on" code="VK_T" shift="on"/>
         <Stroke code="VK_T" meta="on" shift="on"/>
      </Action>
   </Context>
   <Context id="CommandWindow">
      <Action id="kill-line"/>
   </Context>
   <Context id="Explorer">
      <Action id="jump-list-prev"/>
   </Context>
</CustomKeySet>
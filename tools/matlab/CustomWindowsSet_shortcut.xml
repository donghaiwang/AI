<?xml version="1.0" encoding="utf-8"?>
<CustomKeySet derivedfrom="Windows" modifieddefault="false">
   <Context id="Global">
      <Action id="change-current-folder">
         <Stroke alt="on" code="VK_F" shift="on"/>
         <Stroke code="VK_F" meta="on" shift="on"/>
      </Action>
      <Action id="new-mfunction">
         <Stroke alt="on" code="VK_N" shift="on"/>
         <Stroke code="VK_N" meta="on" shift="on"/>
      </Action>
      <Action id="import-data">
         <Stroke code="VK_O" ctrl="on" shift="on"/>
      </Action>
      <Action id="add-cell-divider">
         <Stroke alt="on" code="VK_ENTER" shift="on"/>
         <Stroke code="VK_ENTER" meta="on" shift="on"/>
      </Action>
      <Action id="split-screen-off">
         <Stroke code="VK_F6" shift="on"/>
      </Action>
      <Action id="find-files">
         <Stroke alt="on" code="VK_F" ctrl="on" shift="on"/>
         <Stroke code="VK_F" ctrl="on" meta="on" shift="on"/>
      </Action>
      <Action id="enable-disable-breakpoint">
         <Stroke alt="on" code="VK_F12" shift="on"/>
         <Stroke code="VK_F12" meta="on" shift="on"/>
      </Action>
      <Action id="goto-line">
         <Stroke code="VK_L" ctrl="on"/>
      </Action>
      <Action id="create-cell-from-selection">
         <Stroke alt="on" code="VK_I" shift="on"/>
         <Stroke code="VK_I" meta="on" shift="on"/>
      </Action>
      <Action id="go-until-cursor">
         <Stroke code="VK_F7"/>
      </Action>
      <Action id="show-preferences">
         <Stroke alt="on" code="VK_P" shift="on"/>
         <Stroke code="VK_P" meta="on" shift="on"/>
      </Action>
      <Action id="mlint-fix-action">
         <Stroke code="VK_BACK_QUOTE" ctrl="on"/>
      </Action>
      <Action id="beep">
         <Stroke alt="on" code="VK_B" shift="on"/>
         <Stroke code="VK_B" meta="on" shift="on"/>
      </Action>
      <Action id="clear-workspace">
         <Stroke alt="on" code="VK_K" shift="on"/>
         <Stroke code="VK_K" meta="on" shift="on"/>
      </Action>
      <Action id="clear-all-breakpoints">
         <Stroke code="VK_F12" shift="on"/>
      </Action>
      <Action id="toggle-bookmark">
         <Stroke code="VK_F2" ctrl="on"/>
         <Stroke code="VK_F2" shift="on"/>
      </Action>
      <Action id="inc-search-forward"/>
      <Action id="clear-command-window">
         <Stroke alt="on" code="VK_W" shift="on"/>
         <Stroke code="VK_W" meta="on" shift="on"/>
      </Action>
      <Action id="eval-cell">
         <Stroke code="VK_ENTER" ctrl="on"/>
         <Stroke alt="on" code="VK_ENTER"/>
         <Stroke code="VK_ENTER" meta="on"/>
      </Action>
      <Action id="prev-bookmark">
         <Stroke alt="on" code="VK_F2" shift="on"/>
         <Stroke code="VK_F2" meta="on" shift="on"/>
      </Action>
      <Action id="save-as">
         <Stroke code="VK_S" ctrl="on" shift="on"/>
      </Action>
      <Action id="set-modify-breakpoint-condition">
         <Stroke alt="on" code="VK_F12"/>
         <Stroke code="VK_F12" meta="on"/>
      </Action>
      <Action id="select-previous-tab">
         <Stroke code="VK_PAGE_UP" ctrl="on"/>
         <Stroke code="VK_TAB" ctrl="on" shift="on"/>
      </Action>
      <Action id="prev-cell">
         <Stroke alt="on" code="VK_PAGE_UP"/>
         <Stroke code="VK_PAGE_UP" meta="on"/>
      </Action>
      <Action id="select-next-tab">
         <Stroke code="VK_PAGE_DOWN" ctrl="on"/>
         <Stroke code="VK_TAB" ctrl="on"/>
      </Action>
      <Action id="split-screen-left-right">
         <Stroke alt="on" code="VK_S" shift="on"/>
         <Stroke code="VK_S" meta="on" shift="on"/>
      </Action>
      <Action id="adjust-window-top">
         <Stroke alt="on" code="VK_U" ctrl="on"/>
         <Stroke code="VK_U" ctrl="on" meta="on"/>
      </Action>
      <Action id="publish-to-default-format">
         <Stroke code="VK_P" ctrl="on" shift="on"/>
      </Action>
      <Action id="to-upper-case">
         <Stroke alt="on" code="VK_U" shift="on"/>
         <Stroke code="VK_U" meta="on" shift="on"/>
      </Action>
      <Action id="clear-command-history">
         <Stroke alt="on" code="VK_H" shift="on"/>
         <Stroke code="VK_H" meta="on" shift="on"/>
      </Action>
      <Action id="adjust-window-bottom">
         <Stroke alt="on" code="VK_B" ctrl="on"/>
         <Stroke code="VK_B" ctrl="on" meta="on"/>
      </Action>
      <Action id="exit-debug">
         <Stroke code="VK_F5" shift="on"/>
         <Stroke code="VK_F7" shift="on"/>
      </Action>
      <Action id="open-with-system-browser">
         <Stroke alt="on" code="VK_E" shift="on"/>
         <Stroke code="VK_E" meta="on" shift="on"/>
      </Action>
      <Action id="to-lower-case">
         <Stroke alt="on" code="VK_L" shift="on"/>
         <Stroke code="VK_L" meta="on" shift="on"/>
      </Action>
      <Action id="collapse">
         <Stroke code="VK_COMMA" ctrl="on" shift="on"/>
      </Action>
      <Action id="comment">
         <Stroke code="VK_R" ctrl="on"/>
         <Stroke alt="on" code="VK_SLASH"/>
         <Stroke code="VK_SLASH" meta="on"/>
      </Action>
      <Action id="next-cell">
         <Stroke alt="on" code="VK_PAGE_DOWN"/>
         <Stroke code="VK_PAGE_DOWN" meta="on"/>
      </Action>
      <Action id="new-mclass">
         <Stroke code="VK_N" ctrl="on" shift="on"/>
      </Action>
      <Action id="create-shortcut-from-selection">
         <Stroke alt="on" code="VK_V" shift="on"/>
         <Stroke code="VK_V" meta="on" shift="on"/>
      </Action>
      <Action id="diff-against-autosave">
         <Stroke alt="on" code="VK_C" shift="on"/>
         <Stroke code="VK_C" meta="on" shift="on"/>
      </Action>
      <Action id="uncomment">
         <Stroke code="VK_T" ctrl="on"/>
         <Stroke alt="on" code="VK_SLASH" shift="on"/>
         <Stroke code="VK_SLASH" meta="on" shift="on"/>
      </Action>
      <Action id="debug-continue">
         <Stroke code="VK_F5"/>
         <Stroke code="VK_F8"/>
      </Action>
      <Action id="kill-line">
         <Stroke code="VK_K" ctrl="on"/>
      </Action>
      <Action id="set-path">
         <Stroke alt="on" code="VK_T" shift="on"/>
         <Stroke code="VK_T" meta="on" shift="on"/>
      </Action>
   </Context>
   <Context id="CommandWindow">
      <Action id="kill-line"/>
      <Action id="open-selection">
         <Stroke code="VK_F3"/>
      </Action>
   </Context>
   <Context id="Explorer">
      <Action id="jump-list-prev"/>
      <Action id="find-files"/>
      <Action follows="None" id="open-selection">
         <Stroke code="VK_ENTER"/>
         <Stroke code="VK_D" ctrl="on"/>
         <Stroke code="VK_F3"/>
      </Action>
   </Context>
   <Context id="VariableEditor">
      <Action id="open-selection">
         <Stroke code="VK_F3"/>
      </Action>
   </Context>
   <Context id="Project">
      <Action id="open-selection">
         <Stroke code="VK_ENTER"/>
         <Stroke code="VK_F3"/>
      </Action>
   </Context>
   <Context id="HTMLRenderer">
      <Action id="open-selection">
         <Stroke code="VK_F3"/>
      </Action>
   </Context>
   <Context id="MATLABEditor">
      <Action id="open-selection">
         <Stroke code="VK_F3"/>
      </Action>
   </Context>
   <Context id="Recordlist">
      <Action id="open-selection">
         <Stroke code="VK_F3"/>
      </Action>
   </Context>
</CustomKeySet>